{
  "name": "benx",
  "displayName": "BenX",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:benx.createFile",
    "onCommand:benx.searchOpenFiles"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
