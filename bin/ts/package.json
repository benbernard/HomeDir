    "@aws-sdk/client-dynamodb": "^3.749.0",
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/credential-provider-ini": "^3.744.0",
    "@aws-sdk/lib-dynamodb": "^3.749.0",
    "@aws-sdk/lib-storage": "^3.744.0",
    "@types/luxon": "^3.4.2",
    "@types/mime-types": "^2.1.4",
    "chalk": "^4.1.2",
    "clipboardy": "^4.0.0",
    "jsdom": "^26.0.0",
    "luxon": "^3.5.0",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.7.0",
    "p-limit": "^6.2.0",
