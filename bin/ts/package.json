{
  "name": "bin-ben",
  "version": "1.0.0",
  "main": "index.js",
  "bin": {
    "s3upload": "./dist/s3upload.js",
    "downloader": "./dist/downloader.js",
    "wt-bin": "./dist/wt.js",
    "ic-bin": "./dist/ic.js",
    "git-cleanup-bin": "./dist/git-cleanup.js",
    "git-prune-old-bin": "./dist/git-prune-old.js",
    "claude-notify-bin": "./dist/claude-notify.js",
    "read-tree-bin": "./dist/read-tree.js"
  },
  "scripts": {
    "prebuild": "biome check --apply-unsafe .",
    "build": "tsc --noEmit && node scripts/build.js",
    "build:watch": "concurrently \"tsc --noEmit --watch --preserveWatchOutput\" \"node scripts/build.js --watch\"",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ensure-dev": "ts-node scripts/ensure-dev-setup.ts",
    "dev": "npm run ensure-dev && npm run build:watch"
  },
  "author": "",
  "license": "MIT",
  "description": "",
