    "typescript": "^5.7.3",
    "yargs": "^17.7.2",
    "zx": "^8.8.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.17",
    "@types/node-fetch": "^2.6.12",
    "@types/yargs": "^17.0.33",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.0",
    "tsx": "^4.20.6"
  }
}
