#!/usr/bin/env bash
# Auto-generated wrapper for s3upload
# Checks if source is newer than binary and rebuilds if needed

SOURCE="/Users/benbernard/bin/ts/src/s3upload.ts"
BINARY="/Users/benbernard/bin/ts/dist/s3upload"
BUILD_SCRIPT="/Users/benbernard/bin/ts/scripts/build.ts"

# Check if source is newer than binary
if [[ "$SOURCE" -nt "$BINARY" ]]; then
  echo "Source changed, rebuilding s3upload..." >&2
  cd "/Users/benbernard/bin/ts" && bun run "${BUILD_SCRIPT}" --single "s3upload.ts" >/dev/null 2>&1 || {
    echo "Build failed for s3upload" >&2
    exit 1
  }
fi

# Execute the binary
exec "${BINARY}" "$@"
