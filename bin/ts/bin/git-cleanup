#!/usr/bin/env bash
# Auto-generated wrapper for git-cleanup
# Checks if source is newer than binary and rebuilds if needed

SOURCE="/Users/benbernard/bin/ts/src/git-cleanup.ts"
BINARY="/Users/benbernard/bin/ts/dist/git-cleanup"
BUILD_SCRIPT="/Users/benbernard/bin/ts/scripts/build.ts"

# Check if source is newer than binary
if [[ "$SOURCE" -nt "$BINARY" ]]; then
  echo "Source changed, rebuilding git-cleanup..." >&2
  cd "/Users/benbernard/bin/ts" && bun run "${BUILD_SCRIPT}" --single "git-cleanup.ts" >/dev/null 2>&1 || {
    echo "Build failed for git-cleanup" >&2
    exit 1
  }
fi

# Execute the binary
exec "${BINARY}" "$@"
