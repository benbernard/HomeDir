#!/usr/bin/env bash
# Auto-generated wrapper for close-prs
# Checks if source is newer than binary and rebuilds if needed

SOURCE="/Users/benbernard/bin/ts/src/close-prs.ts"
BINARY="/Users/benbernard/bin/ts/dist/close-prs"
BUILD_SCRIPT="/Users/benbernard/bin/ts/scripts/build.ts"

# Check if source is newer than binary
if [[ "$SOURCE" -nt "$BINARY" ]]; then
  echo "Source changed, rebuilding close-prs..." >&2
  cd "/Users/benbernard/bin/ts" && bun run "${BUILD_SCRIPT}" --single "close-prs.ts" >/dev/null 2>&1 || {
    echo "Build failed for close-prs" >&2
    exit 1
  }
fi

# Execute the binary
exec "${BINARY}" "$@"
