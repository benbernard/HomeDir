#!/usr/bin/env bash
# Auto-generated wrapper for downloader
# Checks if source is newer than binary and rebuilds if needed

SOURCE="/Users/benbernard/bin/ts/src/downloader.ts"
BINARY="/Users/benbernard/bin/ts/dist/downloader"
BUILD_SCRIPT="/Users/benbernard/bin/ts/scripts/build.ts"

# Check if source is newer than binary
if [[ "$SOURCE" -nt "$BINARY" ]]; then
  echo "Source changed, rebuilding downloader..." >&2
  cd "/Users/benbernard/bin/ts" && bun run "${BUILD_SCRIPT}" --single "downloader.ts" >/dev/null 2>&1 || {
    echo "Build failed for downloader" >&2
    exit 1
  }
fi

# Execute the binary
exec "${BINARY}" "$@"
