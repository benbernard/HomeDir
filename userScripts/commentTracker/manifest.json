{
  "manifest_version": 2,

  "name": "Github Comment Tracker",
  "description": "Tracks resolved / unresolved state of comments on github pull requests",
  "version": "1.0",

  "permissions": [
    "https://github.com/"
  ],

  "content_scripts": [{
    "matches": ["*://github.com/*/pull/*"],
    "js": ["jquery-2.1.0.min.js", "underscore-min.js", "parse-1.3.0.min.js", "tracker.js"],
    "css": ["main.css"]
  }]
}
